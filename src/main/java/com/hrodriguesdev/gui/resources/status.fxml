      <Button fx:id="cancelar" alignment="CENTER" layoutX="14.0" layoutY="22.0" mnemonicParsing="false" onAction="#aguardandoOrcamento" prefHeight="45.0" prefWidth="185.0" style="-fx-cursor: hand; -fx-background-radius: 10;" text="Aguardando Orçamento" textAlignment="CENTER">
