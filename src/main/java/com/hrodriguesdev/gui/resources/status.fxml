            <Font size="15.0" />
         </font>
      </Button>
      <Button alignment="CENTER" layoutX="214.0" layoutY="22.0" mnemonicParsing="false" onAction="#aguardandoAprovacao" prefHeight="45.0" prefWidth="185.0" style="-fx-cursor: hand; -fx-background-radius: 10;" text="Aguardando Aprovação" textAlignment="CENTER">
